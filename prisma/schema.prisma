generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id        String   @id @default(cuid())
  name      String
  email     String
  contact   String
  country   String
  proofUrl  String
  createdAt DateTime @default(now())
}
